{
  "compilerOptions": {
    "jsx": "react-jsx",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"],
    "module": "esnext",
    "outDir": "./build",
    "noImplicitAny": true,
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es5",
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "baseUrl": "./src"
  },
  "include": [
    "src",
  ],
  "exclude": ["node-modules"]
}